    mem_wdata_o,        // 写内存数据
    mem_raddr_o,    // 读内存地址
    mem_waddr_o,    // 写内存地址
    mem_we_o,                   // 是否要写内存
    mem_req_o,                  // 请求访问内存标志

    reg_wdata_o,       // 写寄存器数据
     reg_we_o,                   // 是否要写通用寄存器
    reg_waddr_o,   // 写通用寄存器地址

